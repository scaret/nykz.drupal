heavyload ()
{
#ifndef BBSD
    double cpu_load[3];

    get_load (cpu_load);
    if (cpu_load[0] > 15)
        return 1;
    else
        return 0;
#else
#ifdef chkload

    register int load;
    register time_t uptime;

    if (time (0) > uptime)
    {
        load = chkload (load ? TH_LOW : TH_HIGH);
        uptime = time (0) + load + 45;
    }
    return load;
#else

    return 0;
#endif
#endif
}

