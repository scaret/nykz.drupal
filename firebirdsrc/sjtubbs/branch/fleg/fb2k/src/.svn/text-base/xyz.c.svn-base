    char weeknum[7][3] = { "Ìì", "Ò»", "¶ş", "Èı", "ËÄ", "Îå", "Áù" };

    tm = localtime (&now);
    sprintf (datestring, "%4dÄê%02dÔÂ%02dÈÕ%02d:%02d:%02d ĞÇÆÚ%2s",
             tm->tm_year + 1900, tm->tm_mon + 1, tm->tm_mday,
             tm->tm_hour, tm->tm_min, tm->tm_sec, weeknum[tm->tm_wday]);
    return (tm->tm_sec % 10);
}

